EdDesign {
# vty attribute for the frame lines
  dFrameFormat = Attr {
                   attrStyle = Default
                 , attrForeColor = SetTo (ISOColor 2)
                 , attrBackColor = Default
                 , attrURL = Default
                 }
# vty attribute for the status line
, dStatusFormat = Attr {
                    attrStyle = SetTo 32
                  , attrForeColor = SetTo (ISOColor 10)
                  , attrBackColor = Default
                  , attrURL = Default
                  }
# vty attribute for the line numbers to the left
, dLineNoFormat = Attr {
                    attrStyle = SetTo 32
                  , attrForeColor = SetTo (ISOColor 10)
                  , attrBackColor = Default
                  , attrURL = Default
                  }
# Display interval for the line numbers
, dLineNoInterv = 10
# Display interval for the column numbers. Don't set this lower than
# the expected number's length, or strange things might happen.
, dColNoInterval = 40
# vty attribute for the column numbers
, dColNoFormat = Attr {
                   attrStyle = SetTo 32
                 , attrForeColor = SetTo (ISOColor 10)
                 , attrBackColor = Default
                 , attrURL = Default
                 }
# Character to fill the background with
, dBGChar = '\183'
# Character to draw column lines with
, dColChar = Just '|'
# vty attribute for everything in the background
, dBGFormat = Attr {
                attrStyle = Default
              , attrForeColor = SetTo (ISOColor 0)
              , attrBackColor = Default
              , attrURL = Default
              }
# Attribute modifications to apply to the current line
, dCurrLnMod = Attr {
                 attrStyle = Default
               , attrForeColor = Default
               , attrBackColor = SetTo (ISOColor 0)
               , attrURL = Default
               }
# Attribute modifications for bracket matching.
, dBrMod = Attr {
             attrStyle = SetTo 4
           , attrForeColor = Default
           , attrBackColor = Default
           , attrURL = Default
           }
# vty attribute for jump marks
, dJumpMarkFmt = Attr {
                   attrStyle = Default
                 , attrForeColor = SetTo (ISOColor 1)
                 , attrBackColor = Default
                 , attrURL = Default
                 }
# Template for the tab string: first character, padding character,last character.
, dTabStr = ('\183',' ','|')
# vty attributes list for the different character classes
, dCharStyles = [
                  (Whitesp, Attr {
                              attrStyle = Default
                            , attrForeColor = SetTo (ISOColor 4)
                            , attrBackColor = Default
                            , attrURL = Default
                            })
                , (Digit, Attr {
                            attrStyle = Default
                          , attrForeColor = SetTo (ISOColor 1)
                          , attrBackColor = Default
                          , attrURL = Default
                          })
                ,(Lower, Attr {
                           attrStyle = Default
                         , attrForeColor = Default
                         , attrBackColor = Default
                         , attrURL = Default
                         })
                ,(Upper, Attr {
                           attrStyle = Default
                         , attrForeColor = Default
                         , attrBackColor = Default
                         , attrURL = Default
                         })
                ,(Bracket, Attr {
                             attrStyle = Default
                           , attrForeColor = SetTo (ISOColor 3)
                           , attrBackColor = Default
                           , attrURL = Default
                           })
                ,(Operator, Attr {
                              attrStyle = SetTo 32
                            , attrForeColor = SetTo (ISOColor 11)
                            , attrBackColor = Default
                            , attrURL = Default
                            })
                ,(Unprintable, Attr {
                                 attrStyle = SetTo 32
                               , attrForeColor = SetTo (ISOColor 5)
                               , attrBackColor = Default
                               , attrURL = Default
                               })
                ,(Special, Attr {
                             attrStyle = Default
                           , attrForeColor = SetTo (ISOColor 5)
                           , attrBackColor = Default
                           , attrURL = Default
                           })
                ]
# vty attributes list for the different highlight modes
, dHLStyles = [ (HNone, Attr {
                           attrStyle = Default
                         , attrForeColor = Default
                         , attrBackColor = Default
                         , attrURL = Default
                         })
               ,(HBracket, Attr {
                           attrStyle = Default
                         , attrForeColor = Default
                         , attrBackColor = Default
                         , attrURL = Default
                         })
               ,(HComment, Attr {
                             attrStyle = SetTo 32
                           , attrForeColor = SetTo (ISOColor 13)
                           , attrBackColor = Default
                           , attrURL = Default
                           })
              ,(HError, Attr {
                          attrStyle = SetTo 32
                        , attrForeColor = Default
                        , attrBackColor = SetTo (ISOColor 9)
                        , attrURL = Default
                        })
              ,(HKeyword, Attr {
                            attrStyle = Default
                          , attrForeColor = SetTo (ISOColor 2)
                          , attrBackColor = Default
                          , attrURL = Default
                          })
              ,(HSearch, Attr {
                           attrStyle = SetTo 32
                         , attrForeColor = SetTo (ISOColor 9)
                         , attrBackColor = Default
                         , attrURL = Default
                         })
              ,(HSelected, Attr {
                             attrStyle = Default
                           , attrForeColor = SetTo (ISOColor 8)
                           , attrBackColor = SetTo (ISOColor 7)
                           , attrURL = Default
                           })
              ,(HString, Attr {
                           attrStyle = Default
                         , attrForeColor = SetTo (ISOColor 6)
                         , attrBackColor = Default
                         , attrURL = Default
                         })
              ]
}
